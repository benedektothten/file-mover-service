{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AppSettings": {
    "DownloadFolder": "C:\\Users\\yourUser\\Downloads",
    "TempExtensions": [".tmp", ".crdownload", ".part", ".download"],
    "Rules": [
      {
        "Extension": ".pdf",
        "TargetFolder": "G:\\pdfs_to_upload"
      }
    ]
  }
}
